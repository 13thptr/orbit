{
  "name": "spaced-everything-cli",
  "version": "0.0.1",
  "author": "Andy Matuschak",
  "bin": {
    "spaced-everything": "./bin/run"
  },
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.14.0",
    "@oclif/plugin-help": "^2.2.3",
    "@oclif/plugin-spaced-commands": "RasPhilCo/oclif-plugin-spaced-commands",
    "tslib": "^1.10.0",
    "spaced-everything": "0.0.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/node": "~10",
    "globby": "^10.0.2",
    "ts-node": "~8",
    "typescript": "beta"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "license": "UNLICENSED",
  "private": true,
  "main": "dist/index.js",
  "oclif": {
    "commands": "./dist/commands",
    "bin": "spaced-everything",
    "topics": {
      "anki": {"description": "Commands for interacting with Anki"}
    },
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-spaced-commands"
    ]
  },
  "repository": "andymatuschak/",
  "scripts": {
    "build": "tsc -b",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf dist && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
