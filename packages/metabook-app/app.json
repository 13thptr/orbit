{
  "expo": {
    "name": "Orbit",
    "slug": "orbit",
    "privacy": "unlisted",
    "version": "1.0.0",
    "sdkVersion": "37.0.0",
    "assetBundlePatterns": ["**/*"],
    "platforms": ["ios", "android", "web"],
    "packagerOpts": {
      "config": "metro.config.js"
    },
    "ios": {
      "bundleIdentifier": "media.ladder.orbit"
    },
    "android": {
      "package": "media.ladder.orbit"
    },
    "web": {
      "build": {
        "babel": {
          "include": "metabook-client"
        }
      }
    }
  }
}
