{
  "expo": {
    "name": "Orbit",
    "slug": "orbit",
    "privacy": "unlisted",
    "version": "1.0.0",
    "assetBundlePatterns": ["**/*"],
    "platforms": ["ios", "android", "web"],
    "packagerOpts": {
      "config": "metro.config.js"
    },
    "ios": {
      "bundleIdentifier": "com.withorbit.app"
    },
    "android": {
      "package": "com.withorbit.app"
    },
    "web": {
      "build": {
        "babel": {
          "include": "@withorbit/api-client"
        }
      }
    }
  }
}
